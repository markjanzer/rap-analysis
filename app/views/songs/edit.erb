<h1>Edit Song</h1>
<%= form_for @song do |f| %>
  <div class="song-header">
    <h2><%= @song.name %></h2>
    <p><%= @artist_names %></p>
    <p><%= @song.album.title %></p>
  </div>

  <%= render partial: "new_section_form" %>

  <%= render partial: "add_section_button" %>

  <%= render partial: "add_attributes" %>

  <div class="song-edit">
    <%= render partial: 'edit_song_attributes', locals: { song: @song } %>
  </div>
<% end %>